<ul class='events_list'>
  {{#each this}}
    <li class='event_card'>
      <div class='thumb'>
        <img src='{{posterUrl}}' alt='{{name}}' />
      </div>
      <p class='event-title'>{{name}}</p>
      <p class='event-date'>{{dates.start.localDate}}</p>
      <p class='event-location'>{{_embedded.venues.0.name}}</p>
    </li>

  {{/each}}
</ul>